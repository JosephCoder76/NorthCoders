// Please complete this function...
//So we need to create a new array returning every third alements of the array
//Define a new array
//Maybe a slice evey other element
function makeAlternatingArray(array) {

let tempArray = []
    for (let i = 0; i < array.length; i += 2){ //Add two to i every iteration, this means we skip the next 
    //value before we push in to the Array
        tempArray.push(array[i]) //Add the element at index i to a temporary array
    }
    return tempArray
  
}

//makeAlternatingArray(['hey', 'hi']);
// should return ['hey']

//makeAlternatingArray(['a', 'b', 'c', 'd', 'e']);
// should return ['a', 'c', 'e']

makeAlternatingArray([100, 42, 99, 3, -5]);
// should return [100, 99, -5]